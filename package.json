{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "server": "nodemon ./server/index",
    "lint": "eslint --fix --max-warnings=0 --ext .js,.jsx ./src ./server && echo 'Lint complete.'",
    "format:src": "prettier --write \"./src/**/*.{js,jsx}\" && echo 'Format ./src complete.'",
    "format:server": "prettier --write \"./server/**/*.{js,json}\" && echo 'Format ./server complete.'",
    "format:all": "npm run format:src && npm run format:server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "connect-timeout": "^1.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "markdownlint-cli": "^0.30.0",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  }
}
